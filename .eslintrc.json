{
	"extends": "airbnb-base",

	"rules": {
		"no-tabs": 0,
		"newline-per-chained-call": 0,
		"indent": 0,
		"no-undef": 0,
		"no-return-assign": 0,
		"no-underscore-dangle": 0,
		"prefer-destructuring": 0,
		"class-methods-use-this": 0,
		"linebreak-style": 0,
		"no-param-reassign": 0,
		"arrow-parens": 0
	}
}